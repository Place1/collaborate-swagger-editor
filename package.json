  "main": "index.js",
  "scripts": {
    "start": "parcel ./index.html --port 3000",
    "build:yaml": "parcel build ./node_modules/monaco-languages/release/esm/yaml/yaml.js --no-source-maps --out-dir src",
    "build:editor": "parcel build ./node_modules/monaco-editor/esm/vs/editor/editor.worker.js --no-source-maps --out-dir src",
    "postinstall": "npm-run-all --parallel build:yaml build:editor"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "js-yaml": "^3.13.0",
    "monaco-editor": "^0.16.2",
    "monaco-languages": "^1.7.0",
    "peer": "^0.2.10",
    "peerjs": "^0.3.24",
    "peerjs-server": "^0.2.9",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "strict-event-emitter-types": "^2.0.0",
    "swagger-editor": "^3.6.27",
    "swagger-editor-dist": "^3.6.27",
    "swagger-ui": "^3.22.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.1",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.4.1"
  }
